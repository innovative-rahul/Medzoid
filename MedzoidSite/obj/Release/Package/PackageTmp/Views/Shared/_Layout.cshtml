    <script>

        CheckSession();

        function CheckSession() {

            var userString = sessionStorage.getItem('details');
            var user = JSON.parse(userString);

            //var session = getParameterByName('session');

            if (user == null) {
                $('#top_access').show();
                $('#liLoggedin').hide();

            }
            else {
                if (user.userDetails.UserType == "B") {
                    $('#lnkAccount').attr('href', '/User/User/Dashboard');
                }
                else if (user.userDetails.UserType == "D") {
                    $('#lnkAccount').attr('href', '/User/User/Dashboard');
                }
                else if (user.userDetails.UserType == "P") {
                    $('#lnkAccount').attr('href', '/Pharmacy/Pharmacy/Dashboard');
                }
                else if (user.userDetails.UserType == "E") {
                    $('#lnkAccount').attr('href', '/Employee/Employee/Dashboard');
                }
                else if (user.userDetails.UserType == "A") {
                    $('#lnkAccount').attr('href', '/Admin/Admin/Dashboard');
                }



                $('#top_access').hide();
                $('#liLoggedin').show();
                $('#lnkLogin').html('| &nbsp;&nbsp;&nbsp;' + user.userDetails.Name);


                if (window.location.href.includes("Login") || window.location.href.includes("login")) {
                    window.location.href = "/Index";
                }

            }
        }
        LoadDepartment();

        function LoadDepartment() {

            var departments = JSON.parse(localStorage.getItem('departments'))
            if (departments == null) {
                $.ajax({
                    type: 'GET',
                    contentType: "application/json; charset=utf-8",
                    dataType: 'json',
                    url: "/LoadDepartments",
                    headers: { 'Content-type': 'application/x-www-form-urlencoded' },
                    data: {},
                    success: function (data) {
                        if (data.Status == true) {
                            localStorage.setItem('departments', JSON.stringify(data));
                        }
                    },
                    error: function (xhr) {
                        //  alert(xhr.responseText)
                    }
                });
            }
        }

        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, '\\$&');
            var regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, ' '));
        }

    </script>
