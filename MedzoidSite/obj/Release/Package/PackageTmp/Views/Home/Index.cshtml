    <script type="text/javascript" async>
        function Department() {
            $('#Symptom').hide();
            $('#Department').show();
            $('#Disease').hide();
            $('#Procedure').hide();
        }

        function Symptom() {
            $('#Symptom').show();
            $('#Department').hide();
            $('#Disease').hide();
            $('#Procedure').hide();

        }

        function Disease() {
            $('#Symptom').hide();
            $('#Department').hide();
            $('#Disease').show();
            $('#Procedure').hide();

            //https://api.medzoid.com/api/DiseasebySearch?Search=Cancer x-www-form-urlencoded
        }

        function Procedure() {
            $('#Symptom').hide();
            $('#Department').hide();
            $('#Disease').hide();
            $('#Procedure').show();
        }

        function ListPharmacy() {
            var userString = sessionStorage.getItem('details');
            var user = JSON.parse(userString);

            if (user == null) {
                 location.href = "Login?redirectUrl=RegisterPharmacy" ;
            }
            else {
                location.href = "RegisterPharmacy";
            }
        }

        function UploadPrescription() {
            var userString = sessionStorage.getItem('details');
            var user = JSON.parse(userString);

            if (user == null) {
                 location.href = "Login?redirectUrl=UploadPrescription" ;
            }
            else {
                location.href = "UploadPrescription";
            }
        }

        function SearchBloodDonor() {
            var userString = sessionStorage.getItem('details');
            var user = JSON.parse(userString);

            if (user == null) {
                 location.href = "Login?redirectUrl=SearchBloodDonor" ;
            }
            else {
                location.href = "SearchBloodDonor";
            }
        }
        

        $('#search_disease').keyup(function () {
            $('#filter-records').show();
            var html = "";
            var searchField = $(this).val();
            $.ajax({
                type: 'POST',
               // url: 'https://api.medzoid.com/api/DiseasebySearch',
                url: '/DiseasebySearch',
                data: { Search: searchField },
                headers: { 'Content-type': 'application/x-www-form-urlencoded' },
                dataType: 'json',
                success: function (data) {
                    //console.log(data);
                    $.each(data.Data.Data, function (index, element) {
                        html += "<div class=\"rec\"><a onclick=\"getdisease(" + element.id + ",'" + element.name + "')\" >" + element.name + "</a></div>";
                    });
                    $('#filter-records').empty().append(html);
                }
            });
        });

        $('#search_department').keyup(function () {
            $('#filter-department').show();
            var html = "";
            var searchField = $(this).val();

            let searchedDepts = [];
            var departments = JSON.parse(localStorage.getItem('departments'))
            if (departments != null) {
                for (var i = 0; i < departments.Data.length; i++) {
                    if (departments.Data[i].name.toLowerCase().includes(searchField)) {
                        searchedDepts.push(departments.Data[i])
                    }
                }

                if (searchField == '') {
                    searchedDepts = [];
                }
                
                if (searchedDepts.length > 0) {
                    $.each(searchedDepts, function (index, element) {
                        html += "<div class=\"rec\"><a onclick=\"getdep(" + element.id + ",'" + element.name + "')\" >" + element.name + "</a></div>";
                    });
                    $('#filter-department').empty().append(html);
                }
                else {
                    $('#filter-department').empty();
                }
            }

          //Old Logic
            //$.ajax({
            //    type: 'POST',
            //    url: 'https://api.medzoid.com/api/GetDepartmentList',
            //    data: { DepartmentName: searchField },
            //    headers: { 'Content-type': 'application/x-www-form-urlencoded' },
            //    dataType: 'json',
            //    success: function (data) {
            //        console.log(data);

                   

            //        $.each(data.Data, function (index, element) {
            //            html += "<div class=\"rec\"><a onclick=\"getdep(" + element.id + ",'" + element.name + "')\" >" + element.name + "</a></div>";
            //        });
            //        $('#filter-department').empty().append(html);
            //    }

            //});
         
        });

        $('#search_procedure').keyup(function () {
            $('#filter-procedure').show();
            var html = "";
            var searchField = $(this).val();
            $.ajax({
                type: 'POST',
                url: '/GetprocedurebySearch',
                data: { Search: searchField },
                headers: { 'Content-type': 'application/x-www-form-urlencoded' },
                dataType: 'json',
                success: function (data) {
                    console.log(data);
                    $.each(data.Data.Data, function (index, element) {
                        html += "<div class=\"rec\"><a onclick=\"getprocedure(" + element.id + ",'" + element.Name + "')\" >" + element.Name + "</a></div>";
                    });
                    $('#filter-procedure').empty().append(html);
                }

            });
        });


        function bodysymtoms(id, name) {
            location.href = "/symptoms?BodyPartId=" +id + "&Search="+name;
        }

        function getdisease(id, name) {
            $('#search_disease').val(name);
            $('#hidden_did').val(id);
            $('#filter-records').hide();
            $('#diseased').submit();
        }

        function getdep(id, name) {
            $('#search_department').val(name);
            $('#hidden_dep_id').val(id);
            $('#filter-department').hide();
            $('#depat').submit();
        }

        function getprocedure(id, name) {
            $('#search_procedure').val(name);
            $('#hidden_pid').val(id);
            $('#filter-procedure').hide();
            $('#proced').submit();
        }

        var tpj = jQuery;
        var revapi72;
        tpj(document).ready(function () {
            $('#Symptom').hide();
            $('#Department').show();
            $('#Disease').hide();
            $('#Procedure').hide();
            if (tpj("#rev_slider_72_1").revolution == undefined) {
                revslider_showDoubleJqueryError("#rev_slider_72_1");
            } else {
                revapi72 = tpj("#rev_slider_72_1").show().revolution({
                    sliderType: "standard",
                    jsFileLocation: "rev-slider-files/js/",
                    sliderLayout: "auto",
                    dottedOverlay: "none",
                    delay: 9000,
                    navigation: {
                        keyboardNavigation: "off",
                        keyboard_direction: "horizontal",
                        mouseScrollNavigation: "off",
                        mouseScrollReverse: "default",
                        onHoverStop: "off",
                        touch: {
                            touchenabled: "on",
                            touchOnDesktop: "off",
                            swipe_threshold: 75,
                            swipe_min_touches: 1,
                            swipe_direction: "horizontal",
                            drag_block_vertical: false
                        }
                        ,
                        arrows: {
                            style: "gyges",
                            enable: true,
                            hide_onmobile: true,
                            hide_under: 560,
                            hide_onleave: true,
                            hide_delay: 200,
                            hide_delay_mobile: 1200,
                            tmp: '',
                            left: {
                                h_align: "left",
                                v_align: "center",
                                h_offset: 20,
                                v_offset: 0
                            },
                            right: {
                                h_align: "right",
                                v_align: "center",
                                h_offset: 20,
                                v_offset: 0
                            }
                        }
                    },
                    visibilityLevels: [1240, 1024, 778, 480],
                    gridwidth: 1240,
                    gridheight: 600,
                    lazyType: "none",
                    shadow: 0,
                    spinner: "spinner0",
                    stopLoop: "off",
                    stopAfterLoops: -1,
                    stopAtSlide: -1,
                    shuffle: "off",
                    autoHeight: "off",
                    disableProgressBar: "on",
                    hideThumbsOnMobile: "off",
                    hideSliderAtLimit: 0,
                    hideCaptionAtLimit: 0,
                    hideAllCaptionAtLilmit: 0,
                    debugMode: false,
                    fallbacks: {
                        simplifyAll: "off",
                        nextSlideOnWindowFocus: "off",
                        disableFocusListener: false,
                    }
                });
            }
        });	/*ready*/
    </script>

}
